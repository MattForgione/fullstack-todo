{
  "extends": ["plugin:cypress/recommended", "../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "rules": {
    "cypress/no-unnecessary-waiting": "off"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"]
    },
    {
      "files": ["*.json"],
      "rules": {
        "semi": "off"
      }
    }
  ]
}
